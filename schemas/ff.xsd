				<xs:element minOccurs="0" name="link" nillable="true" type="xs:string"/>
				<xs:element minOccurs="0" name="isShownAt" nillable="true" type="xs:string"/>
				<xs:element name="documentType" type="ff:conceptComplexType"/>
				<xs:element minOccurs="0" name="persons">
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="person" type="ff:personComplexType"/>
