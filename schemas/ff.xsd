<?xml version="1.0" encoding="utf-8" standalone="no"?>
<xs:schema xmlns:ff="http://www.kulturarv.dk/fundogfortidsminder/ff" xmlns:gml="http://www.opengis.net/gml" xmlns:lido="http://www.lido-schema.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.kulturarv.dk/fundogfortidsminder/ff">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/03/xml.xsd"/>
	<xs:import namespace="http://www.lido-schema.org" schemaLocation="http://www.lido-schema.org/schema/v1.0/lido-v1.0.xsd"/>
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/feature.xsd"/>

	<xs:complexType name="identifierComplexType">
		<xs:annotation>
			<xs:documentation>
				How to record: There is no controlled list of
				identifier types.
				Suggested values include, but are not limited to
				the following: doi
				(Digital Objects Identifier), uid (Globally unique
				identifier), hdl
				(Handle), localpermalinkpurl (Persistent Uniform
				Resource Locator),
				uri (Uniform Resource Identifier), url (Uniform
				Resource Locator),
				urn (Uniform Resource Name)
      </xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="type" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="conceptComplexType">
		<xs:annotation>
			<xs:documentation>Definition: Set for identifiers and terms of a
				concept.</xs:documentation>
			<xs:documentation>How to record: A concept describes a conceptual
				resource. Concepts are organized in concept schemes like thesauri,
				classification schemes, taxonomies, subject-heading systems, or any
				other type of structured controlled vocabulary. See also SKOS
				specifications at http://www.w3.org/2004/02/skos/</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="conceptID" type="ff:identifierComplexType">
				<xs:annotation>
					<xs:documentation>Definition: A unique identifier for the concept.
					</xs:documentation>
					<xs:documentation>How to record: Preferably taken from and linking
						to a published controlled vocabulary.</xs:documentation>
					<xs:documentation>Notes: All FBB controlled vocabularies will be
						published as SKOS on URIs of the form
						http://www.kulturarv.dk/fbb/skos/$vocab#$id.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="term" type="xs:string">
				<xs:annotation>
					<xs:documentation>Definition: A name for the referred concept, used
						for indexing.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!-- Elements for rightsWork. -->

	<xs:complexType name="webResourceComplexType">
		<xs:annotation>
			<xs:documentation>Definition: A uri/url reference to a web resource
				that describes / represents the item, e.g. a metadata record.
			</xs:documentation>
			<xs:documentation>Notes: It differs from an identifier for the item
				itself. </xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="pref" type="xs:string">
					<xs:annotation>
						<xs:documentation>Definition: Qualifies the value as a preferred
							or
							alternative variant. </xs:documentation>
						<xs:documentation>How to record: Data values: preferred, alternate
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="formatResource" type="xs:string">
					<xs:annotation>
						<xs:documentation>Definition: Indicates the internet media type,
							e.g. the file format of the given web resource.
						</xs:documentation>
						<xs:documentation>How to record: Data values should be taken from
							the official IANA list (see
							http://www.iana.org/assignments/media-types/). Includes:
							text/html, text/xml, image/jpeg, audio/mpeg, video/mpeg,
							application/pdf.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute ref="xml:lang"/>
				<xs:attribute name="label" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="legalBodyRefComplexType">
		<xs:annotation>
			<xs:documentation>Definition: Reference information to a legal body.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="legalBodyID" type="ff:identifierComplexType">
				<xs:annotation>
					<xs:documentation>Definition: Unambiguous identification of the
						institution or person referred to as legal body.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="legalBodyName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Definition: Name of the institution or person.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="legalBodyWeblink" type="ff:webResourceComplexType">
				<xs:annotation>
					<xs:documentation>Definition: Weblink of the institution or person
						referred to as legal body.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:attribute name="sortorder" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Definition: Assigns a priority order for online
				presentation of the element. </xs:documentation>
			<xs:documentation>How to record: Has to be a positive integer, with
				descending priority from 1 to x. </xs:documentation>
		</xs:annotation>
	</xs:attribute>

	<xs:complexType name="rightsComplexType">
		<xs:annotation>
			<xs:documentation>Definition: Information about rights management;
				may include copyright and other intellectual property statements.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="rightsType" type="ff:conceptComplexType">
				<xs:annotation>
					<xs:documentation>Definition: The specific type of right being
						recorded. </xs:documentation>
					<xs:documentation>How to record: For example: copyright,
						publication right, data protection right, trademark.Preferably
						taken from a published controlled value list. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="rightsDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Definition: The date on which a right is or was
						current.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="rightsHolder">
				<xs:annotation>
					<xs:documentation>Definition: The holder of the right.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ff:legalBodyRefComplexType">
							<xs:attribute ref="ff:sortorder"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="creditLine" type="lido:textComplexType">
				<xs:annotation>
					<xs:documentation>Definition: Acknowledgement of the rights
						associated with the physical and/or digital object as requested.
					</xs:documentation>
					<xs:documentation>How to record: Repeat this element only for
						language variants.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="rightsWorkWrap">
		<xs:annotation>
			<xs:documentation>Definition: Wrapper for rights information about
				the object / work described.</xs:documentation>
			<xs:documentation>Notes: Rights information for the record and for
				resources is recorded in the respective rights elements recordRights
				and rightsResource. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" minOccurs="0" name="rightsWorkSet">
					<xs:annotation>
						<xs:documentation>Definition: Information about rights management;
							may include copyright and other intellectual property statements
							about the object / work.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="ff:rightsComplexType">
								<xs:attribute ref="ff:sortorder"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="personComplexType">
		<xs:complexContent>
			<xs:extension base="ff:conceptComplexType">
				<xs:sequence>
					<xs:element minOccurs="0" name="title" nillable="true" type="ff:conceptComplexType"/>
					<xs:element name="role" type="ff:conceptComplexType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="characteristicComplexType">
		<xs:sequence>
			<xs:element minOccurs="0" name="objectName" nillable="true" type="xs:string"/>
			<xs:element minOccurs="0" name="preservationYear" nillable="true" type="xs:integer"/>
			<xs:element minOccurs="0" name="attributeYear" nillable="true" type="xs:integer"/>
			<xs:element minOccurs="0" name="measurementYear" nillable="true" type="xs:integer"/>
			<xs:element minOccurs="0" name="compassDirection" nillable="true" type="xs:string"/>
			<xs:element minOccurs="0" name="lengthCm" nillable="true" type="xs:double"/>
			<xs:element minOccurs="0" name="widthCm" nillable="true" type="xs:double"/>
			<xs:element minOccurs="0" name="heightCm" nillable="true" type="xs:double"/>
			<xs:element minOccurs="0" name="depthCm" nillable="true" type="xs:double"/>
			<xs:element minOccurs="0" name="areaM2" nillable="true" type="xs:double"/>
			<xs:element minOccurs="0" name="preservationState" nillable="true" type="ff:conceptComplexType"/>
			<xs:element minOccurs="0" name="attribute" nillable="true" type="ff:conceptComplexType"/>
			<xs:element minOccurs="0" name="measurementType" nillable="true" type="ff:conceptComplexType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="objectComplexType">
		<xs:sequence>
			<xs:element name="id" type="ff:identifierComplexType"/>
			<xs:element minOccurs="0" name="text" nillable="true" type="xs:string"/>
			<xs:element name="date">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fromYear" type="xs:integer"/>
						<xs:element name="toYear" type="xs:integer"/>
						<xs:element minOccurs="0" name="mainPeriod" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="period">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ff:conceptComplexType">
										<xs:sequence>
											<xs:element name="publicTerm" type="xs:string"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="characteristics">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="characteristic" nillable="true" type="ff:characteristicComplexType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="objectType">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ff:conceptComplexType">
							<xs:sequence>
								<xs:element name="objectClassTerm" type="xs:string"/>
								<xs:element name="objectClassExplanation" type="xs:string"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="transportObject" nillable="true">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="name" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="constructionId" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="salvaged" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="departureDate" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="destination" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="constructionSite" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="constructionYear" nillable="true" type="xs:integer"/>
						<xs:element minOccurs="0" name="homePort" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="owner" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="cargo" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="displacement" nillable="true" type="xs:integer"/>
						<xs:element minOccurs="0" name="material" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="rigging" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="engine" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="armament" nillable="true" type="xs:string"/>
						<xs:element minOccurs="0" name="wreckedYear" nillable="true" type="xs:integer"/>
						<xs:element minOccurs="0" name="transportType" type="ff:conceptComplexType"/>
						<xs:element minOccurs="0" name="displacementType" type="ff:conceptComplexType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="object" type="ff:objectComplexType">
		<xs:annotation>
			<xs:documentation>Anlaeg</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="digitalDocumentation">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="ff:identifierComplexType"/>
				<xs:element minOccurs="0" name="name" nillable="true" type="xs:string"/>
				<xs:element name="date" type="xs:string"/>
				<xs:element minOccurs="0" name="filename" nillable="true" type="xs:string"/>
				<xs:element minOccurs="0" name="copyright" nillable="true" type="xs:string"/>
				<xs:element minOccurs="0" name="legalUse" nillable="true" type="xs:string"/>
