					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="persons">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="person" type="ff:personComplexType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

