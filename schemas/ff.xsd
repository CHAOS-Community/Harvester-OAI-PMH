	<xs:element name="site">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="ff:identifierComplexType"/>
				<xs:element name="date" type="xs:dateTime"/>
				<xs:element name="siteNo" type="xs:integer"/>
				<xs:element name="ext" type="xs:string"/>
				<xs:element name="parishNo" type="ff:conceptComplexType"/>
				<xs:element name="plotType" type="ff:conceptComplexType"/>
				<xs:element minOccurs="0" name="placeNames">
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="placeName" type="ff:placeNameComplexType"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element minOccurs="0" name="otherIds">
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="otherId" type="ff:otherIdComplexType"/>
