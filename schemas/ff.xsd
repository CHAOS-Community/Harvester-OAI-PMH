							<xs:element name="listingNo" type="xs:string"/>
							<xs:element name="listingStatus" type="xs:string"/>
							<xs:element minOccurs="0" name="listingText" nillable="true" type="xs:string"/>
							<xs:element minOccurs="0" name="registrationDate" nillable="true" type="xs:dateTime"/>
							<xs:element name="registrationStatus" type="xs:string"/>
