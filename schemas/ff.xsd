						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element minOccurs="0" name="guide">
					<xs:complexType>
						<xs:sequence>
							<xs:element minOccurs="0" name="internalAssessment" nillable="true" type="xs:string"/>
							<xs:element minOccurs="0" name="externalAssessment" nillable="true" type="xs:string"/>
							<xs:element minOccurs="0" name="attractionText" nillable="true" type="xs:string"/>
							<xs:element minOccurs="0" name="signSite" nillable="true" type="xs:boolean"/>
							<xs:element minOccurs="0" name="signRoad" nillable="true" type="xs:boolean"/>
							<xs:element minOccurs="0" name="parking" nillable="true" type="xs:boolean"/>
							<xs:element minOccurs="0" name="publicAccess" nillable="true" type="xs:boolean"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element minOccurs="0" name="preservationState">
					<xs:complexType>
						<xs:sequence>
							<xs:element minOccurs="0" name="surroundingsYear" nillable="true" type="xs:integer"/>
							<xs:element minOccurs="0" name="threatYear" nillable="true" type="xs:integer"/>
							<xs:element minOccurs="0" name="signYear" nillable="true" type="xs:integer"/>
							<xs:element minOccurs="0" name="surroundings" nillable="true" type="ff:conceptComplexType"/>
							<xs:element minOccurs="0" name="threat" nillable="true" type="ff:conceptComplexType"/>
							<xs:element minOccurs="0" name="monumentSign" nillable="true" type="ff:conceptComplexType"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element minOccurs="0" name="adminArea">
